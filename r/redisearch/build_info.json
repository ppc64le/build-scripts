{
  "maintainer": "Vinod.K1@ibm.com",
  "package_name": "redisearch",
  "github_url": "https://github.com/RediSearch/RediSearch",
  "version": "v2.8.13",
  "default_branch": "master",
  "package_dir": "r/redisearch",
  "docker_cmd": "docker build -t ${package_name}:$PACKAGE_VERSION ${dir}",
  "build_script": "redisearch_ubi_9.3.sh",
  "validate_build_script": "true",
  "use_non_root_user": "false",
  "docker_build": "true",
  "v2.0.*": {
