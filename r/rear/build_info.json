{
  "maintainer": "AbhiIBM",
  "package_name": "rear",
  "github_url": "https://github.com/rear/rear",
  "version": "rear-2.7",
  "default_branch": "master",
  "build_script": "rear_ubi_9.3.sh",
  "package_dir": "r/rear",
  "docker_build": true,
  "docker_cmd": "docker build -t ${package_name}:$PACKAGE_VERSION ${dir}",
  "use_non_root_user": false,
  "validate_build_script": true,
  "rear-2.*": {
    "dir": "ubi9.3",
    "build_script": "rear_ubi_9.3.sh"
  },
  "*": {
    "dir": "ubi9.3",
    "build_script": "rear_ubi_9.3.sh"
  }
}
