{
  "maintainer": "vinodk99",
  "package_name": "vault",
  "github_url": "https://github.com/hashicorp/vault.git",
  "version": "v1.16.2",
  "default_branch": "main",
  "docker_cmd": "docker build -t ${package_name}:$PACKAGE_VERSION ${dir}",
  "package_dir": "v/vault/",
  "build_script": "vault_ubi_9.3.sh",
  "validate_build_script": true,
  "use_non_root_user": "false",
  "docker_build": true,
  "v1.11.*": {
    "dir": "1.11.2-ubi8",
    "args": {
      "VAULT_VERSION": "$PACKAGE_VERSION",
      "GO_VERSION": "1.17.12"
    }
  },
  "v1.12.*": {
    "dir": "1.11.2-ubi8",
    "args": {
      "VAULT_VERSION": "$PACKAGE_VERSION",
      "GO_VERSION": "1.19.6"
    }
  },
  "v1.13.*,v1.14.*": {
    "dir": "1.16.2_ubi9",
    "build_script": "vault_ubi_9.3.sh",
    "args": {
      "VAULT_VERSION": "$PACKAGE_VERSION",
      "GO_VERSION": "1.22.1"
    }
  },
  "v1.*.*": {
    "dir": "1.13.1-ubi8",
    "args": {
      "VAULT_VERSION": "$PACKAGE_VERSION",
      "GO_VERSION": "1.21.7"
    }
  },
  "v1.5.*": {
    "dir": "1.5.3-ubi8",
    "args": {
      "VAULT_VERSION": "$PACKAGE_VERSION",
      "GO_VERSION": "1.14.7"
    }
  },
  "v1.7.*": {
    "dir": "1.7.0-ubi8",
    "args": {
      "VAULT_VERSION": "$PACKAGE_VERSION",
      "GO_VERSION": "1.16.7"
    }
  },
  "v1.8.*": {
    "dir": "1.8.5-ubi8",
    "args": {
      "VAULT_VERSION": "$PACKAGE_VERSION",
      "GO_VERSION": "1.17.3"
    }
  },
  "v1.9.*": {
    "dir": "1.9.1-ubi8",
    "args": {
      "VAULT_VERSION": "$PACKAGE_VERSION",
      "GO_VERSION": "1.17.13"
    }
  },
  "v0.10.*": {
    "dir": "0.10.4",
    "args": {
      "VAULT_VERSION": "$PACKAGE_VERSION",
      "GO_VERSION": "1.10.3"
    }
  },
   "v*.*.*": {
    "dir": "1.16.2_ubi9",
    "build_script": "vault_ubi_9.3.sh",
    "args": {
      "VAULT_VERSION": "$PACKAGE_VERSION",
      "GO_VERSION": "1.22.1"
    }
  }
}
