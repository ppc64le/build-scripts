    "package_name": "zipkin",
    "github_url": "https://github.com/openzipkin/zipkin",
    "version": "zipkin-0.7.0",
    "default_branch": "master",
    "package_dir": "z/zipkin",
    "docker_cmd": "docker build -t ${package_name}:$PACKAGE_VERSION ${dir}",
    "build_script": "zipkin_ubi_8_7.sh",
