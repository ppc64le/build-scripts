  "package_name": "cytoolz",
  "github_url": "https://github.com/pytoolz/cytoolz.git",
  "version": "0.12.0",
  "default_branch": "master",
  "wheel_build" : true,
  "package_dir": "c/cytoolz",
  "docker_cmd": "docker build -t ${package_name}:$PACKAGE_VERSION ${dir}",
  "build_script": "cytoolz_ubi_9.3.sh",
