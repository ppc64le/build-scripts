  "use_non_root_user": "false",
  "docker_build": "false",
  "0.12.2": {
    "dir": "ubi8.7",
    "build_script": "cytoolz_ubi_8.7.sh"
  },
  "*": {
    "dir": "ubi9.3",
    "build_script": "cytoolz_ubi_9.3.sh"
  }
}
