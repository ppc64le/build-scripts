{
    "package_name" : "opentelemetry-go",
    "github_url":"https://github.com/open-telemetry/opentelemetry-go",
    "version": "v1.11.1",
    "default_branch": "main",
    "package_dir": "o/opentelemetry-go",
    "docker_build": false,
    "validate_build_script": true,
    "use_non_root_user": false,
    "build_script" : "opentelemetry-go_ubi_8.5.sh",

    "v1.*.*" : {
            "args": {
                "GO_VERSION":"1.18.5"
        }
    },
    
    "v0.*.*" : {
            "args": {
                "GO_VERSION":"1.17.4"
        }
    }
}
